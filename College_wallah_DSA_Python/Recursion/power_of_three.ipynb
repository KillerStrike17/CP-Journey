    "class Solution:\n",
    "    def isPowerOfThree(self, n: int) -> bool:\n",
    "        if n ==0 or n==2:\n",
    "            return False\n",
    "        if n==1:\n",
    "            return True\n",
    "        if n%3==1 or n%3==2:\n",
    "            return False\n",
    "        return self.isPowerOfThree(n//3)\n",
    "        "
